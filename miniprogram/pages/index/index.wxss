.index_swiper swiper {
  height: 340rpx;
}
.index_swiper swiper image {
  width: 100%;
}
.index_category {
  display: flex;
  padding: 20rpx;
}
.index_category navigator {
  flex: 1;
}
.index_category navigator image {
  width: 100%;
}
.index_floor .floor_group .floor_title image {
  width: 100%;
}
.index_floor .floor_group .floor_items {
  overflow: hidden;
}
.index_floor .floor_group .floor_items navigator {
  float: left;
  width: 33.33%;
  box-sizing: border-box;
}
.index_floor .floor_group .floor_items navigator:nth-last-child(-n+4) {
  height: 27.73vw;
  border-left: 10rpx solid #fff;
}
.index_floor .floor_group .floor_items navigator:nth-last-child(-n+2) {
  border-top: 10rpx solid #fff;
}
.index_floor .floor_group .floor_items navigator image {
  width: 100%;
  height: 100%;
}
